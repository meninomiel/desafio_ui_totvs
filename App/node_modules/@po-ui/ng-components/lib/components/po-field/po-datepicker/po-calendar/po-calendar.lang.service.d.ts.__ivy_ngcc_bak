export declare class PoCalendarLangService {
    months: {
        pt: string;
        en: string;
        es: string;
    }[];
    shortWeekDays: {
        pt: string;
        en: string;
        es: string;
    }[];
    wordMonth: {
        pt: string;
        en: string;
        es: string;
    };
    wordYear: {
        pt: string;
        en: string;
        es: string;
    };
    lang: string;
    constructor();
    setLanguage(lang: string): void;
    getWordMonth(): any;
    getWordYear(): any;
    getMonth(month: number): any;
    getArrayMonths(): any[];
    getWeedDays(day: number): any;
    getArrayWeekDays(): any[];
}
